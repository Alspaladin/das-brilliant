.page
  margin 0 auto
  width 100%
  height 100%
  background-position center center
  background-attachment fixed
  background-size cover

.page_header
  margin-bottom 37px
  padding-top 32px
  font-family 'Didact Gothic', sans-serif
  font-size 30px
  font-weight 300
  text-align center

.page_wrapper
  padding: 140px 0 60px;

.page_container
  position relative
  z-index 1
  min-height 100%
